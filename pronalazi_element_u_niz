#include <stdio.h>
#include <string.h>
#include <math.h>



int main()
{
  int array[10] = { 2, 34, 56, 23, 65, 21, 67, 99, 45, 62 };

  int broj;
  printf("Unesi elemtent koji trazis \n");
  scanf("%d",&broj);
    int k=0;
  for(int i=0; i<10;i++){
    if(array[i]==broj){
        k=k+1;
    }else{
        k=k+0;
  }

}
    if(k>=1){
    printf("Element je pronadjen");
  }else{
        printf("Element nije pronadjen");
    }


}
