#include <stdio.h>
#include <stdlib.h>

int main()
{

    int broj=199;
    int n;
    int b=0;
    int s=0;
    while(broj>0){
        n= broj%10;
        broj=broj/10;
        b= b+n;
    }
    if(b>=10){
        while(b>0){
            n=b%10;
            b=b/10;
            s=s+n;
        }
    if(s==10){
        s=1+0;
    }
     printf("%d",s);
    }else{
        printf("%d",b);
    }
}

